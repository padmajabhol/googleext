{
    "name": "Krypto Knights",
    "version": "1.0",
    "description": "Block social media sites to boost up ur productivity.",
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "matches": [
                "*://*.reddit.com/*",
                "*://*.instagram.com/*",
                "*://*.facebook.com/*",
                "*://*.twitter.com/*"
            ],
            "css": [
                "block.css"
            ],
            "js": [
                "block.js"
            ]
        }
    ],
    "manifest_version": 2
}